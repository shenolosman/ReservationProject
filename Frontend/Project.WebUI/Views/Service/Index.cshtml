@model List<ResultServiceDto>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}
<h1>Service</h1>

<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Service Title</th>
        <th>Description</th>
        <th>Delete</th>
        <th>Update</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <th>@item.ServiceId</th>
            <td>@item.Title</td>
            <td>@item.Description</td>
            <td><a href="/Service/DeleteService/@item.ServiceId" class="btn btn-danger">Delete</a></td>
            <td><a href="/Service/UpdateService/@item.ServiceId" class="btn btn-success">Update</a></td>
        </tr>
    }
</table>
<a href="/Service/AddService/" class="btn btn-info">New</a>